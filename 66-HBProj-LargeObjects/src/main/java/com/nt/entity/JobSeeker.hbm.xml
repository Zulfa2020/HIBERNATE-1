<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping  package="com.nt.entity">
   <class name="JobSeeker"  table="JOBSEEKER">
        <id name="jsId" type="int">
             <generator class="increment"/>
        </id>
        <property name="jsName"  type="string" length="15"/>
        <property name="jsAddrs" type="string" length="15"/>
        <property name="photo"/>
        <property name="resume" />
   <!--      <property name="active" type="boolean"/> -->
        
   </class>
 </hibernate-mapping>